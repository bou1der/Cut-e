    "chai": "^4.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^6.2.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1"
  }
